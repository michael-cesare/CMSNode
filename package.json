{
  "name": "cms-node",
  "description": "node server that manipulates data between CMS and frontend.",
  "version": "0.0.1",
  "private": true,
  "author": "Michael Cesare",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon",
    "tsc": "tsc",
    "prebuild": "rimraf r dist && rimraf typings",
    "build": "tsc -p . && npm run html",
    "test": "npm run build",
    "prepack": "npm i && npm run build",
    "html": "copyfiles pages/**/*.html mock/**/*.json public/**/*.* dist/"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@controllers": "dist/api/controllers",
    "@webControllers": "dist/web/controllers",
    "@helpers": "dist/helpers",
    "@middleware": "dist/middleware",
    "@repositories": "dist/repositories",
    "@services": "dist/services",
    "@srcTypes": "dist/types",
    "@mocks": "dist/mocks",
    "@util": "dist/util"
  },
  "dependencies": {
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.17.8",
    "@types/express-http-proxy": "^1.6.1",
    "@types/node": "^14.0.1",
    "@types/uuid": "^8.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "isomorphic-fetch": "^3.0.0",
    "module-alias": "^2.2.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.35",
    "@types/node-fetch": "^2.5.7",
    "copyfiles": "^2.4.0",
    "rimraf": "3.0.2",
    "tslib": "^2.0.3",
    "nodemon": "^1.18.9",
    "ts-node": "^7.0.1",
    "typescript": "^4.0.3"
  }
}
